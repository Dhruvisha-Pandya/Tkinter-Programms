    "from tkinter import *\n",
    "root=Tk()\n",
    "root.geometry(\"600x400\")\n",
    "\n",
    "def call1():\n",
    "    print(\"Welcome to C\")\n",
    "\n",
    "f1=Frame(root,bg=\"red\",borderwidth=6)\n",
    "f1.pack(side=LEFT,anchor=\"nw\")\n",
    "\n",
    "b1=Button(root, fg=\"red\",text=\"Start C\", command=call1)\n",
    "b1.pack(side=LEFT,padx=23)\n",
    "\n",
    "root.mainloop()"
